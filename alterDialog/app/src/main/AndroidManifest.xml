<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:andriod="http://schemas.android.com/apk/distribution">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlterDialog" />
    <activity andriod:name = "FirstActivity" andriod:exported = "true">
        <intent-filter>
            <category andriod:name "andriod.intent.category.LUNCHER"/>
            <action andriod:name="andriod.intent.action.MAIN"/>
        </intent-filter>
    </activity>
</application>

</manifest>